{
  "name": "@nockered/sdk",
  "version": "0.0.1",
  "license": "MIT",
  "description": "A simple Docker SDK for Node.JS",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "./dist/"
  ],
  "engines": {
    "node": ">=16.13"
  },
  "scripts": {
    "build": "rimraf ./dist && tsc -p tsconfig.build.json",
    "test": "cross-env NODE_NO_WARNINGS=1 NODE_OPTIONS='--loader=ts-node/esm' DOCKER_DAEMON_URL=unix:/var/run/docker.sock:/v1.41 c8 ava",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "got": "^13.0.0"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^5.0.0",
    "@types/node": "^20.9.0",
    "ava": "^5.1.1",
    "c8": "^8.0.1",
    "cross-env": "^7.0.3",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "ava": {
    "files": [
      "!tests/**/TestExecutionContext.*"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--no-warnings",
      "--experimental-specifier-resolution=node"
    ]
  },
  "repository": "https://github.com/Nockered/sdk",
  "author": "Yann SEGET <dev@leafgard.fr>",
  "keywords": [
    "node",
    "docker",
    "sdk"
  ],
  "private": false
}
