{
  "name": "node-docker-sdk",
  "version": "0.2.3",
  "license": "MIT",
  "description": "A simple Docker SDK for Node.JS",
  "exports": "./dist/src/index.js",
  "types": "dist/src",
  "type": "module",
  "files": [
    "dist/src"
  ],
  "engines": {
    "node": ">=16.13"
  },
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "test": "ava"
  },
  "dependencies": {
    "got": "^12.0.1"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^2.0.0",
    "@types/node": "^16.11.26",
    "ava": "^4.0.1",
    "esbuild-node-loader": "^0.6.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5"
  },
  "ava": {
    "files": [
      "tests/**/*"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--no-warnings",
      "--loader=esbuild-node-loader",
      "--experimental-specifier-resolution=node"
    ]
  },
  "repository": "https://github.com/Leafgard/node-docker-sdk.git",
  "author": "Yann SEGET <dev@leafgard.fr>",
  "keywords": [
    "node",
    "docker",
    "sdk"
  ],
  "private": false
}
