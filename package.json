{
  "name": "@nockered/sdk",
  "version": "0.2.3",
  "license": "MIT",
  "description": "A simple Docker SDK for Node.JS",
  "exports": "./dist/src/index.js",
  "types": "dist/src",
  "type": "module",
  "files": [
    "dist/src"
  ],
  "engines": {
    "node": ">=16.13"
  },
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "test-windows": "cross-env DOCKER_DAEMON_URL=http://localhost:2375/v1.41 c8 ava",
    "test-unix": "cross-env DOCKER_DAEMON_URL=unix:/var/run/docker.sock:/v1.41 c8 ava"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "got": "^12.0.1"
  },
  "devDependencies": {
    "@esbuild-kit/esm-loader": "^2.5.4",
    "@sindresorhus/tsconfig": "^3.0.1",
    "@types/node": "^18.0.0",
    "ava": "^5.1.1",
    "c8": "^7.11.3",
    "cross-env": "^7.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^4.1.2",
    "typescript": "^4.7.4"
  },
  "ava": {
    "files": [
      "tests/**/*"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--no-warnings",
      "--loader=@esbuild-kit/esm-loader",
      "--experimental-specifier-resolution=node"
    ]
  },
  "repository": "https://github.com/Nockered/sdk",
  "author": "Yann SEGET <dev@leafgard.fr>",
  "keywords": [
    "node",
    "docker",
    "sdk"
  ],
  "private": false
}
